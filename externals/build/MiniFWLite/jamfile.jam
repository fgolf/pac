# jamfile for CMS2 mini FWLite

lib libMiniFWLite
  : # sources
    #$(MINIFWLITE_PATH)/TaSBanner.cc
    [
      rootcint Dict
        : # requirements
          <include>$(MINIFWLITE_PATH)
          <header>DataFormats.h
          <header>Wrapper.h
          <header>EDProduct.h
          <header>Math/LorentzVector.h
          <header>Math/PositionVector3D.h
          <header>TBits.h
          <linkdef>MiniFWLiteLinkDef.h
    ]
    # dependent packages
    /ROOT
  : # requirements 
    <link>shared
    <include>$(MINIFWLITE_PATH)
    <include>$(ROOT_PATH)/include
  : # default-build
  : # usage-requirements
    <include>$(MINIFWLITE_PATH)
  ;

#default-install libMiniFWLite ;
